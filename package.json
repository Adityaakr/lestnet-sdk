{
  "name": "@lestnet/sdk",
  "version": "0.1.0",
  "description": "Official SDK for interacting with the Lestnet blockchain",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "examples"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "test": "vitest",
    "test:watch": "vitest watch",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run build"
  },
  "keywords": ["blockchain", "ethereum", "lestnet", "sdk", "web3"],
  "author": "Lestnet Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/lestnet/sdk"
  },
  "dependencies": {
    "ethers": "^6.13.5"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
